{
    "name": "ashiqfardus/horizon-running-jobs",
    "description": "Monitor currently running jobs in Laravel Horizon for distributed systems",
    "keywords": [
        "laravel",
        "horizon",
        "queue",
        "jobs",
        "monitoring",
        "redis",
        "distributed"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ashiq Fardus",
            "email": "ashiqfardus@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/redis": "^9.0|^10.0|^11.0|^12.0",
        "laravel/horizon": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ashiqfardus\\HorizonRunningJobs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashiqfardus\\HorizonRunningJobs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashiqfardus\\HorizonRunningJobs\\HorizonRunningJobsServiceProvider"
            ],
            "aliases": {
                "RunningJobs": "Ashiqfardus\\HorizonRunningJobs\\Facades\\RunningJobs"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}

